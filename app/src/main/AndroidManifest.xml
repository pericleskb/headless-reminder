<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.joyfulDonkey.headlessreminder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.joyfulDonkey.headlessreminder.ui.dashboard.DashboardActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name="com.joyfulDonkey.headlessreminder.broadcastReceivers.ScheduleAlarmsReceiver"/>
        <receiver android:name="com.joyfulDonkey.headlessreminder.broadcastReceivers.RingAlarmReceiver"/>
    </application>

</manifest>